generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model problem {
  id         Int      @id @default(autoincrement())
  title      String
  difficulty String
  created_at DateTime @default(now())
  updated_at DateTime
}
